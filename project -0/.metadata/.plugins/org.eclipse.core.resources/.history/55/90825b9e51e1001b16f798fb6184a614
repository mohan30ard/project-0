package eg1;

public class count {

	public static void main(String[] args) {
		int arr[]= {6,7,9,8,6};
		int max =arr[0];
		for (int i = 0; i <arr.length; i++) {
			
			if(arr[i]>max) {
				max=arr[i];
				
			}
		}
		for (int k = 0; k < arr.length; k++) {
			
		
		for (int j = 0; j < arr.length; j++) {
			int ele=0;
			if(arr[j]!=max)
			{
				ele=arr[j];
				ele++;
				arr[j]=ele;
			}
			System.out.println(arr[j]);
		}
		}
		for (int k = 0; k < arr.length; k++) {
			//System.out.println(arr[k]);
		}

	}

}
