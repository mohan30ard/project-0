<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>bank_thor_v2</display-name>
  <welcome-file-list>
    <welcome-file>welcome.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>BankCrudController</display-name>
    <servlet-name>BankCrudController</servlet-name>
    <servlet-class>com.bank.controller.BankCrudController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BankCrudController</servlet-name>
    <url-pattern>/bank</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CustomerLoginController</display-name>
    <servlet-name>CustomerLoginController</servlet-name>
    <servlet-class>com.bank.controller.CustomerLoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CustomerLoginController</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SuccessController</display-name>
    <servlet-name>SuccessController</servlet-name>
    <servlet-class>com.bank.controller.SuccessController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SuccessController</servlet-name>
    <url-pattern>/success</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RegistrationsuccessController</display-name>
    <servlet-name>RegistrationsuccessController</servlet-name>
    <servlet-class>com.bank.controller.RegistrationsuccessController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationsuccessController</servlet-name>
    <url-pattern>/rsuccess</url-pattern>
  </servlet-mapping>
</web-app>